          <mxGeometry height="460" width="120" x="360" y="30" as="geometry" />
        </mxCell>
        <mxCell id="9o7GIDE-S3LhQBWQ07d4-96" parent="9o7GIDE-S3LhQBWQ07d4-95" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=top;rounded=0;fontFamily=Helvetica;fontSize=11;fontColor=default;fontStyle=0;labelBackgroundColor=none;" value="&lt;div&gt;Уровень:&lt;/div&gt;&lt;div&gt;&lt;b&gt;&lt;span style=&quot;color: rgb(255, 0, 0);&quot;&gt;критический&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Некачественные политики безопасности могут породить пробелы в защите.&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Некачественные платежные данные приведут к невозможности обработки платежей ЖКУ и сделок.&lt;/div&gt;" vertex="1">
          <mxGeometry height="410" width="120" y="40" as="geometry" />
        </mxCell>
