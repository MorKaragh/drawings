        <mxCell id="9o7GIDE-S3LhQBWQ07d4-89" parent="9o7GIDE-S3LhQBWQ07d4-88" style="swimlane;startSize=40;html=1;fontFamily=Helvetica;fontSize=11;labelBackgroundColor=none;fillColor=#d0cee2;strokeColor=#56517e;" value="Утечка" vertex="1">
